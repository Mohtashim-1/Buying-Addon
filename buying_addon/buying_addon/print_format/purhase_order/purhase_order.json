 "html": "<meta charset=\"utf-8\">\r\n\t\t<!--<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>-->\r\n\r\n<style type=\"text/css\">\r\n*{ font-size:12px;}\r\n.tab2 tr { \r\n\tline-height:0;\r\n}\r\ntable tr { \r\n    overflow: hidden;\r\n    height: 14px;\r\n    white-space: nowrap;\r\n\t\r\n}\r\n@media print\r\n{\r\n  table { page-break-after:auto }\r\n  tr    { page-break-inside:avoid; page-break-after:auto }\r\n  td    { page-break-inside:avoid; page-break-after:auto }\r\n  thead { display:table-header-group }\r\n  tfoot { display:table-footer-group }\r\n}\r\n\r\n\r\n.address-heading-center {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:center;\r\n}\r\n.address-heading-center-white {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:center;\r\n\tcolor:#FFF;\r\n}\r\n.address-heading-left {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:left;\r\n}\r\n.address-heading-left-white {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:left;\r\n\tcolor:#FFF;\r\n}\r\n.heading {\tfont-family: \"Arial Black\", Gadget, sans-serif;\r\n\tfont-size: 20px;\r\n\tfont-weight: bold;\r\n\ttext-align:center;\r\n\tcolor:076ba0;\r\n}\r\n\r\n.normal-content {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:left;\r\n}\r\n.normal-content-center {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:13px;\r\n\ttext-align:center;\r\n}\r\n\r\n.normal-content-bold-center {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:13px;\r\n\ttext-align:center;\r\n\tfont-weight:bold;\r\n}\r\n.normal-content-bold-left {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:12px;\r\n\ttext-align:left;\r\n\tfont-weight:bold;\r\n}\r\n.normal-content-bold-right {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:10px;\r\n\ttext-align:right;\r\n\tfont-weight:bold;\r\n}\r\n#border-bottom {\t\r\nborder-bottom:solid 3px;\r\nborder-bottom-color:#E8E8E8;\r\n\r\n\r\n}\r\n#border-bg {\t\r\n\r\nbackground-color:#E8E8E8;\r\n\r\n\r\n}\r\n#border-bottom-light {\t\r\nborder-bottom:solid 2px;\r\nborder-bottom-color:#076ba0;\r\n\r\n\r\n}\r\n#border-bottom-grey {\t\r\nborder-bottom:solid 2px;\r\nborder-bottom-color:#CCC;\r\n\r\n\r\n}\r\n#border-bottom-right {\t\r\nborder-right:solid 2px;\r\nborder-right-color:#076ba0;\r\n\r\n\r\n}\r\n.head {font-weight:bold; padding:-1px;}\r\n.style1 {font-family: Arial, Helvetica, sans-serif; font-size: 11px; font-weight: bold; text-align: center; color: #FFF; }\r\n.style2 {\r\n    font-family: Arial, Helvetica, sans-serif;\r\n    font-size: 11px;\r\n    font-weight: bold;\r\n    text-align: left;\r\n    color: #FFF;\r\n    background: #076ba0 !important;\r\n  }\r\n\r\n.tab2 tr:nth-child(2n+1) {    background:#E8E8E8;           }\r\n.style3 {font-family: Arial, Helvetica, sans-serif; font-size: 14px; text-align: left; font-weight: bold; }\r\n.style4 {font-family: Arial, Helvetica, sans-serif; font-size: 14px; text-align: left; }\r\n.style5 {font-size: 13px}\r\n.heading-right {\tfont-family: \"Arial Black\", Gadget, sans-serif;\r\n\tfont-size: 25px;\r\n\tfont-weight: bold;\r\n\ttext-align:right;\r\n\tcolor:076ba0;\r\n}\r\n#border-bottom {\t\r\nborder-bottom:solid 3px;\r\nborder-bottom-color:#076ba0;\r\n\r\n\r\n}\r\n.address-heading {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n}\r\n.address {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:13px;\r\n}\r\n.heading-right {\tfont-family: \"Arial Black\", Gadget, sans-serif;\r\n\tfont-size: 25px;\r\n\tfont-weight: bold;\r\n\ttext-align:right;\r\n\tcolor:076ba0;\r\n}\r\n</style>\r\n\r\n<table width=\"100%\">\r\n  <tr>\r\n       <td id=\"border-bottom\"><img src=\"https://erp.sahexports.com/files/SAH_logo.png\" width=\"200px\"><br>\r\n    <!--<td id=\"border-bottom\"><img src=\"https://sahexports.frappe.cloud/files/Capture.PNG\" width=\"200px\"><br>-->\r\n      <span class=\"address-heading\">(HOME TEXTILE DIVISION)</span><br><span class=\"address\">Plot # :4,5,6,13,14,15, Sector B-IX, <br>\r\nKarachi Export Processing Zone,<br>\r\nKarachi 75150 Pakistan.<br>\r\nPh 021-35080157<br>\r\nWebsite : www.sahent.com</span></td>\r\n    <td width=\"40%\" class=\"heading-right\" id=\"border-bottom\">Purchase Order<br>{{doc.name}}</td>\r\n  </tr>\r\n\r\n</table>\r\n\r\n\r\n<br>\r\n\r\n<table width=\"100%\" border=\"0\" height=\"70\">\r\n  <tr>\r\n   <td  class=\"style3\">Vendor :</td>\r\n   <td>{{doc.supplier}}</td>\r\n   <td  class=\"style3\">P.O. Date :</td>\r\n   <td>{{doc.get_formatted('transaction_date')}}</td>\r\n  </tr>\r\n  <tr>\r\n   <td class=\"style3\">Contact Person :</td>\r\n   <td></td>\r\n\r\n   <td  class=\"style3\">Delivery :</td>\r\n   <td>{{doc.get_formatted('schedule_date') or ''}}</td>\r\n  </tr>\r\n</table>\r\n\r\n\r\n<table width=\"100%\" align=\"left\" border=\"1\">\r\n  <tr>\r\n      <td width=\"10%\" class=\"style2\" id=\"border-bg\"><div align=\"center\">S #</div></td>\r\n    <td width=\"10%\" class=\"style2\" id=\"border-bg\"><div align=\"center\">Item</div></td>\r\n    <td width=\"18%\" class=\"style2\" id=\"border-bg\"><div align=\"center\">Unit</div></td>\r\n    <td width=\"11%\" class=\"style2\" id=\"border-bg\"><div align=\"center\">Quantity</div></td>\r\n    <td width=\"11%\" class=\"style2\" id=\"border-bg\"><div align=\"center\">Price</div></td>\r\n    <td width=\"11%\" class=\"style2\" id=\"border-bg\"><div align=\"center\">Amount</div></td>\r\n  </tr>\r\n\r\n  {%- for row in doc.items %}\r\n    <tr>\r\n        <td class=\"normal-content-center\" id=\"border-bottom-grey\"><div align=\"left\"><span class=\"normal-content-bold-center\">{{ row.idx }}</span></div></td>\r\n      <td class=\"normal-content-center\" id=\"border-bottom-grey\"><div align=\"left\"><span class=\"normal-content-bold-center\">{{ row.item_code }}</span></div></td>\r\n      <td class=\"normal-content-bold-left\" id=\"border-bottom-grey\"><span class=\"normal-content\">{{ row.uom }}</span></td>\r\n      <td class=\"normal-content-bold-left\" id=\"border-bottom-grey\"><span class=\"normal-content\">{{ row.qty }}</span></td>\r\n      <td class=\"normal-content-bold-left\" id=\"border-bottom-grey\"><span class=\"normal-content\">{{ row.base_net_rate }}</span></td>\r\n      <td class=\"normal-content-bold-left\" id=\"border-bottom-grey\"><span class=\"normal-content\">{{ row.base_net_amount }}</span></td>\r\n    </tr>\r\n  {%- endfor %}\r\n\r\n  \r\n    <tr>\r\n      <td colspan=\"2\" class=\"style2\" id=\"border-bg\" align=\"center\"><strong>Total Qty</strong></td>\r\n      <td class=\"normal-content-bold-left\" ><span class=\"normal-content\">{{ doc.total_qty }}</span></td>\r\n      \r\n      <td colspan=\"2\" class=\"style2\" id=\"border-bg\" align=\"center\"><strong>Total Amount</strong></td>\r\n      <td class=\"normal-content-bold-left\"><span class=\"normal-content\">{{ doc.grand_total }}</span></td>\r\n        \r\n\r\n    </tr>\r\n\r\n  \r\n</table>\r\n\r\n\r\n<table width=\"100%\" > \r\n<tr>\r\n<td width=\"7%\">\r\n</td>\r\n<td width=\"93%\"></td>\r\n</tr>\r\n</table>\r\n\r\n\r\n<table width=\"100%\" align=\"center\">\r\n    <tr>\r\n        <td>\r\n            <h1>Note:</h1>\r\n        </td>\r\n    </tr>\r\n    <tr>\r\n        <td>1. Please do not proceed production with out approved purchase order.</td><br>\r\n        \r\n    </tr>\r\n    <tr>\r\n        \r\n        <td>2. Please always mention job number on all delivery challans.</td><br/>\r\n        \r\n    </tr>\r\n    <tr>\r\n        \r\n        <td>3. Please do not deliver excess quantity with out approval.</td><br/>\r\n    </tr>\r\n    \r\n  <tr>\r\n    <td class=\"normal-content-bold-center\" width=\"20%\"><br></td>\r\n    <td width=\"20%\">&nbsp;</td>\r\n    <td width=\"20%\"></td>\r\n    <td width=\"20%\"></td>\r\n    <td width=\"20%\">&nbsp;</td>\r\n  </tr>\r\n</table>\r\n\r\n<table width=\"100%\" align=\"center\">\r\n  <tr>\r\n \r\n    <td class=\"normal-content-bold-center\" colspan=\"2\">\r\n          <p id=\"border-bottom\" class=\"normal-content-bold-center\" width=\"20%\"></p>\r\n        Prepared By<br>\r\n     {{ frappe.get_fullname(doc.owner) }}<br>\r\n     {{ doc.get_formatted('creation') }}\r\n    </td>\r\n    <td>&nbsp;</td>\r\n    \r\n    <td>&nbsp;</td>\r\n    <td class=\"normal-content-bold-center\">\r\n        \r\n          \r\n      {% if doc.workflow_state == \"Approved\" %}\r\n      <p id=\"border-bottom\" class=\"normal-content-bold-center\" width=\"20%\"></p>\r\n        {% set approve_user, approve_date = frappe.db.get_value(\"Comment\", {\"reference_name\": doc.name, \"content\": \"Approved\"}, [\"owner\", \"creation\"]) %}\r\n        {% if approve_user and approve_date %}\r\n          Approved By<br> \r\n          {{ frappe.get_fullname(approve_user) }}<br>\r\n          {{ approve_date.strftime('%d %b %Y %H:%M:%S') }}\r\n        {% else %}\r\n          <p>No approval details found.</p>\r\n        {% endif %}\r\n      {% else %}\r\n        &nbsp;\r\n      {% endif %}\r\n    </td>\r\n    \r\n    {% if doc.supplier != \"LOCAL PURCHASE\" %}\r\n<td class=\"normal-content-bold-center\" colspan=\"2\">\r\n    <p id=\"border-bottom\" class=\"normal-content-bold-center\" width=\"20%\"></p>\r\n    Vendor Confirmation<br>\r\n    Signature<br>\r\n</td>\r\n{% endif %}\r\n  </tr>\r\n</table>\r\n",
